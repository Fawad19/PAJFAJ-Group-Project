<%- include ../partials/header.ejs %>

<%- include ../partials/main_nav.ejs %>
  

  <!-- Main Content -->
  <main class="container-survey">
    <video autoplay loop muted plays-inline class="back-video">
      <source src="/images/video.mp4" type="video/mp4">
      </video>
    <div>
        <div><br>
            <h1 class="text-center">Survey 2 Result</h1>
            
            <% if(typeof displayName == 'string' && displayName) { %>
                
            <% } %><br>
            
            <br>
            <table class="table table-bordered table-striped">
                <!-- Header Row-->
                <tr>
                    <% if(typeof displayName == 'string' && displayName) { %>
                        <th class="text-center">Budget</th>
                        <th class="text-center">AreaCode</th>                        
                        <th class="text-center">Housing</th>
                        
                    <% } else { %>
                        <th class="text-center">Budget</th>
                        <th class="text-center">AreaCode</th>                        
                        <th class="text-center">Housing</th>
                        <th></th>
                        <th></th>
                    <% } %>
                </tr>

                <!-- Repeatable Template Row -->

                <% for(let count = 0; count < SurveyList2.length; count++) { %>
                    <tr>
                        <% if(typeof displayName == "string" && displayName) { %>
                           <td class="text-center"><%= SurveyList2[count].budget %></td>
                           <td class="text-center"><%= SurveyList2[count].areaCode %></td>
                           <td class="text-center"><%= SurveyList2[count].housing %></td>
                           
                
                           <td class="text-center">
                            <a href="/surveylist2/edit2/<%= SurveyList2[count]._id %>" class="btn btn-primary"> <i class="fa"></i>Edit</a>
                          </td>
                          <td class="text-center">
                            <a href="/surveylist2/delete/<%= SurveyList2[count]._id %>" class="btn btn-warning btn-sm"> <i class="fa"></i>Delete</a>
                          </td>
                         <% } else { %>
                            <td class="text-center"><%= SurveyList2[count].budget %></td>
                            <td class="text-center"><%= SurveyList2[count].areaCode %></td>
                            <td class="text-center"><%= SurveyList2[count].housing %></td>
                         <% } %>
                       </tr>
                <% } %>
            </table>
        </div>
    </div>
  </main>
 
<%- include ../partials/bottom_nav.ejs %>  

<%- include ../partials/footer.ejs %>